<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.longge.thirdpartdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- EventBus -->
        <activity
            android:name=".eventbus.EventBusActivity"
            android:label="EventBus" />
        <activity
            android:name=".eventbus.EventBus2Activity"
            android:label="EventBus2" />

        <!-- greenDao -->
        <activity
            android:name=".greendao.GreenDaoActivity"
            android:label="GreenDao" />
        <activity android:name=".eventbus.StickyActivity" />
        <activity android:name=".eventbus.FragmentTestActivity"></activity>
    </application>

</manifest>