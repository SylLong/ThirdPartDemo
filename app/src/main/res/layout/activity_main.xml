<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:id="@+id/activity_main"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
              android:paddingBottom="@dimen/activity_vertical_margin"
              android:paddingLeft="@dimen/activity_horizontal_margin"
              android:paddingRight="@dimen/activity_horizontal_margin"
              android:paddingTop="@dimen/activity_vertical_margin"
              tools:context="com.longge.thirdpartdemo.MainActivity">

    <Button
        android:id="@+id/btn_eventBus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EventBus"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_greenDAO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="greenDAO"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_dagger2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="dagger2"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_webSocket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="webSocket"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_webSocket2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="webSocket2"
        android:textAllCaps="false" />
</LinearLayout>
